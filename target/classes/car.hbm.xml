<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gta.cardealer.models.Car" table="mvc.cars"
		schema="mvc">
		<id name="vin" type="java.lang.String" column="vin">
			<generator class="assigned"></generator>
		</id>
		<property name="make" type="java.lang.String" column="make"></property>
		<property name="model" type="java.lang.String" column="model"></property>
		<property name="year" type="java.lang.String" column="year"></property>
	</class>

</hibernate-mapping>